{
  "name": "flickity/yii2-flickity",
  "description": "Touch, responsive, flickable carousils",
  "type": "yii2-extension",
  "keywords": [
    "flickity",
    " slider",
    " carousel",
    " "
  ],
  "license": "MIT",
  "authors": [
    {
      "name": " Akhmadov Dev",
      "email": "akhmadovdev@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2": "~2.0",
    "bower-asset/flickity": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "akhmadovdev\\flickity\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/akhmadov-dev/yii2-flickity.git"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
